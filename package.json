    "watch": "npm-scripts-watcher"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmcminn/ol.git"
  },
  "bugs": {
    "url": "https://github.com/bmcminn/ol/issues"
  },
  "author": "Brandtley McMinn",
  "license": "MIT",
  "homepage": "https://github.com/bmcminn/ol#readme",
  "watch": {
    "src/**/*.styl": [
      "css"
    ],
    "src/**/*.js": [
      "build"
    ],
    "views/**/*.hbs": [
      "templates"
    ]
  },
  "dependencies": {
    "axios": "^0.12.0",
    "handlebars": "^4.0.5",
    "jquery": "^3.0.0",
    "lodash": "^4.13.1",
