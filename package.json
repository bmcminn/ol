{
  "name": "ownlocal-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "bootstrap": "node bin/bootstrap",
    "serve": "node bin/serve --spa",
    "build": "webpack",
    "css": "stylus src/stylus/main.styl -o public/css/main.css",
    "templates": "handlebars ./views -f ./src/js/template-files.js -c handlebars/runtime -e hbs",
