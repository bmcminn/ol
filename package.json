    "grunt": "^1.0.1",
    "json-loader": "^0.5.4",
    "nightmare": "^2.5.2",
    "node-static": "^0.7.7",
    "npm-scripts-watcher": "^1.0.2",
    "script-loader": "^0.7.0",
    "stylus": "^0.54.5",
    "webpack": "^1.13.1"
